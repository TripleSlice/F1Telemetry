<src:BaseSettingView  
    x:TypeArguments="src1:InputTelemetryViewModel"
    xmlns:src="clr-namespace:F1T.MVVM.Views"
    xmlns:src1="clr-namespace:F1T.MVVM.ViewModels" 
    x:Class="F1T.MVVM.Views.InputTelemetry.InputTelemetrySettingView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:F1T.MVVM.Views.InputTelemetry"
    xmlns:converters="clr-namespace:F1T.Converters"
    xmlns:themes="clr-namespace:F1T.Themes"
    mc:Ignorable="d" 
    d:DesignHeight="450" d:DesignWidth="800">


    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <src:CommonSettingUserControl ToggleModule="ToggleVisibilityButton_Click" 
                                      OpacitySliderValue="{Binding OpacitySliderValue}"
                                      MaxFPSValue="60" MinFPSValue="1"
                                      FPSValue="{Binding FramesPerSecond, Mode=TwoWay}"
                                      ></src:CommonSettingUserControl>
        
    </Grid>
</src:BaseSettingView>
